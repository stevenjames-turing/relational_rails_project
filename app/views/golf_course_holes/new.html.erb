<%= form_with url: "/golf_courses/#{@course.id}/holes", method: :post, local: true do |form| %>
  <p><%= form.label :name %>
  <%= form.text_field :name %></p>
  <p><%= form.label :hazard? %>
  <%= form.select :hazard, ['pick one below', 'true', 'false'] %></p>
  <p><%= form.label :par%>
  <%= form.number_field :par %></p>
  <%= form.submit "Create Hole"%>
<% end %>