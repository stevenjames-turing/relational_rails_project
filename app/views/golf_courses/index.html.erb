<h1>Golf Courses</h1>
<% @golf_courses.each do |course| %>
<p>
    Course Name: <%= course.name %>
    (<%= course.created_at %>) 
    <%= link_to 'Edit', "/golf_courses/#{course.id}/edit" %>
    <%= link_to "Delete", "/golf_courses/#{course.id}", method: :delete %>
    </p>
<% end %>
<a href="/golf_courses/new">New Golf Course</a>