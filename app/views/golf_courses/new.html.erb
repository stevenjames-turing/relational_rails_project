<form action="/golf_courses" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter your new golf course:
  <p>Golf Course Name:
  <input type='text'name='name'/><br/>
  </p>

  <p>Public Course?
  <select id="public" name="public">
    <option value="pick one">pick one below</option>
    <option value=true>true</option>
    <option value=false>false</option>
  </select>
  </p>

  <p>How many holes does your course have?
  <input type="integer" name="hole_count">
  </p>

  <input type='submit' name="Submit"/>
</form>